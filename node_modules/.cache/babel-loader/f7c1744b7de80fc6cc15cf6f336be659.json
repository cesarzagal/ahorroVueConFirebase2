[sortKey]) {\n          compare = 1;\n        } else if (b[sortKey] > a[sortKey]) {\n          compare = -1;\n        }\n        return compare;\n      });\n      state.estado=true;\n      state.lista=jobs;\n    },\n\n  },\n  actions: {\n    async fetchList({commit}){\n      try {\n        const snapshot = await db.collection('ahorro').orderBy('fecha').get();\n        snapshot.forEach(doc=> {\n          let dataTemp= doc.data();\n          commit('Add_Item', {\n            date : dataTemp.fecha,\n            value: dataTemp.monto\n          })\n        });\n      }\n      catch (e) {\n        //console.log(e)\n      }\n    },\n    async storeItem({commit}, payload){\n      try {\n        await db.collection('ahorro').add({\n          fecha: payload.fecha,\n          monto: payload.monto,\n        });\n        commit('Add_Item', {\n          date : payload.fecha,\n          value: payload.monto\n        });\n        commit('Sort_Items', 'date')\n      }\n      catch (e) {\n        //console.log(e)\n      }\n    },\n  },\n  modules: {\n  }\n})\n"]}]}